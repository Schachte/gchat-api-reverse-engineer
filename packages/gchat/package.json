{
  "name": "gchat-cli",
  "version": "1.0.0",
  "description": "Google Chat CLI client",
  "main": "dist/index.js",
  "bin": {
    "gchat": "./dist/cli.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsx src/cli.ts",
    "dev:watch": "tsx watch src/cli.ts",
    "start": "node dist/cli.js",
    "serve": "node dist/cli.js api",
    "serve:watch": "tsx watch --clear-screen=false src/cli.ts api",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts"
  },
  "keywords": [
    "google-chat",
    "cli",
    "client"
  ],
  "license": "MIT",
  "dependencies": {
    "@mherod/get-cookie": "^4.4.1",
    "better-sqlite3": "^12.6.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dotenv": "^16.6.1",
    "node-fetch": "^3.3.2",
    "protobufjs": "^7.2.6",
    "playwright-core": "^1.58.2",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.18.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
